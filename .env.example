# 电影管理项目环境变量配置模板
# 复制此文件为 .env 并填入实际值

# ===================
# 数据库配置
# ===================
DB_HOST=localhost
DB_PORT=3306
DB_USER=root
DB_PASSWORD=your_database_password
DB_NAME=movie

# ===================
# Redis 配置
# ===================
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_DB=0
REDIS_PASSWORD=your_redis_password

# ===================
# Jellyfin 配置
# ===================
JELLYFIN_API_URL=http://127.0.0.1:8096
JELLYFIN_API_KEY=your_jellyfin_api_key
JELLYFIN_USER_ID=your_jellyfin_user_id
JELLYFIN_ITEM_ID=your_jellyfin_item_id
JELLYFIN_PLAYLISTS_ID=your_jellyfin_playlists_id

# ===================
# 下载客户端配置
# ===================
QB_HOST=127.0.0.1
QB_PORT=6363
QB_USERNAME=admin
QB_PASSWORD=your_qbittorrent_password

# ===================
# 代理配置
# ===================
PROXY_SECRET=your_proxy_secret

# ===================
# 应用配置
# ===================
APP_ENV=development  # development, production, test
APP_DEBUG=true
APP_SECRET_KEY=your_app_secret_key